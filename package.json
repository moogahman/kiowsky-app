{
    "name": "kiosk-app",
    "version": "1.0.0",
    "description": "Kiosk app",
    "type": "module",
    "private": "true",
    "scripts": {
        "build": "vite build",
        "build:server": "pnpm run functions build",
        "dev": "concurrently \"pnpm run dev:client\" \"pnpm run dev:server\"",
        "dev:client": "vite",
        "dev:server": "pnpm run functions dev",
        "preview": "vite preview",
        "lint": "pnpm exec eslint \"src/**/*.{ts,tsx}\"",
        "lint:fix": "pnpm exec eslint \"src/**/*.{ts,tsx}\" --fix",
        "format": "pnpm exec prettier src/**/* --write",
        "test": "pnpm run functions test",
        "test:coverage": "pnpm run functions test:coverage",
        "functions": "pnpm --filter functions",
        "dotenv-vault": "pnpm dlx dotenv-vault@latest"
    },
    "keywords": [
        "mcdonalds",
        "maccas",
        "kiosk",
        "system"
    ],
    "author": "moogahman",
    "license": "GPL-3.0-only",
    "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a",
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@types/eslint": "^8.56.11",
        "@types/node": "^20.16.1",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "concurrently": "^8.2.2",
        "eslint": "~9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-react-refresh": "^0.4.11",
        "firebase-tools": "^13.15.4",
        "globals": "^15.9.0",
        "jsdom": "^24.1.3",
        "prettier": "^3.3.3",
        "tsx": "^4.19.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^7.18.0",
        "vite": "^5.4.2",
        "vite-plugin-env-compatible": "^2.0.1",
        "vite-tsconfig-paths": "^5.0.1"
    },
    "dependencies": {
        "dotenv": "^16.4.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.3.0",
        "react-router-dom": "^6.26.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "pnpm": {
        "overrides": {
            "ws@>=6.0.0 <6.2.3": ">=6.2.3"
        }
    }
}
